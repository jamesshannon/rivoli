{
  "name": "webui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "proto-build": "rm -rf src/lib/rivoli/protos && buf generate ../protos/"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.15.0-1",
    "@bufbuild/protoc-gen-es": "^1.1.1",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.2.0",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "@bufbuild/protobuf": "^1.1.1",
    "@mac-barrett/svelte-data-table": "^0.1.1",
    "bson": "^5.1.0",
    "carbon-components-svelte": "^0.73.4",
    "carbon-icons-svelte": "^11.4.0",
    "carbon-pictograms-svelte": "^12.3.0",
    "celery-node": "^0.5.9",
    "elkjs": "^0.8.2",
    "fuse.js": "^6.6.2",
    "mongodb": "^5.1.0",
    "svelte-markdown": "^0.2.3",
    "svelvet": "^7.0.13"
  }
}
